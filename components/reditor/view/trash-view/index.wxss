/* components/reditor/trash-view/index.wxss */
/* 废纸篓页面提示框 */
.tips-container {
  width: 710rpx;
  height: 42px;
  box-sizing: border-box;
  margin: auto;
  padding: 0 26rpx;
  display: flex;
  align-items: center;
  background-color: #efefef;
  border-radius: 16rpx;
} 
.tips-icon {
  width: 14px;
  height: 14px;
}
.tips-text {
  margin-left: 12px;
  font-size: 12px;
  color: #9d9d9d;
}
.tips-text-num {
  font-family: 'DIN Alternate';
}