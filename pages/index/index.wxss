/**index.wxss**/
@import "/templates/loading/index.wxss";
page {
  background-color: #f9f9f9;
}
/* 顶部导航栏 */
.nav-bar-logo {
  width: 110px;
  height: 32px;
}
/* 底部悬浮按钮 */
.btn-container {
  position: fixed;
  width: 100%;
  display: flex;
  padding-bottom: 20px;
  justify-content: center;
  z-index: 100;
}
.add-note-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 52px;
  height: 52px;
  background: #FD3B44;
  box-shadow: 0px 4px 20px rgba(253, 59, 68, 0.25);
  border-radius: 10px;
}
.add-note-icon {
  width: 20px;
  height: 20px;
}

